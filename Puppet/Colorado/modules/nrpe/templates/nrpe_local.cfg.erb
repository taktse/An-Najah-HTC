# -*- conf-unix -*-

# system
command[check_kernel] = /bin/uname -a
command[check_load] = <%= @nrpe_plugins %>/check_load -w 15,10,5  -c 30,25,20
command[check_load_hot] = <%= @nrpe_plugins %>/check_load -w 30,20,10 -c 60,50,40
command[check_load_hot_and_spicy] = <%= @nrpe_plugins %>/check_load -w 60,40,20 -c 120,100,80
command[check_memory] = <%= @nrpe_plugins %>/check_memory -C -u -w 75 -c 90
command[check_memory_hot] = <%= @nrpe_plugins %>/check_memory -C -u -w 80 -c 95
command[check_memory_hot_and_spicy] = <%= @nrpe_plugins %>/check_memory -C -u -w 85 -c 98
command[check_process] = <%= @nrpe_plugins %>/check_procs -w 500 -c 750
command[check_process_hot] = <%= @nrpe_plugins %>/check_procs -w 1000 -c 1500
command[check_process_hot_and_spicy] = <%= @nrpe_plugins %>/check_procs -w 2000 -c 3000
command[check_swap] = <%= @nrpe_plugins %>/check_swap -w 20% -c 10%
command[check_swap_hot] = <%= @nrpe_plugins %>/check_swap -w 70% -c 50%
command[check_swap_hot_and_spicy] = <%= @nrpe_plugins %>/check_swap -w 70% -c 70%
command[check_time] = <%= @nrpe_plugins %>/check_ntp_peer -H localhost
command[check_users] = <%= @nrpe_plugins %>/check_users -w 10 -c 20
command[check_users_hot] = <%= @nrpe_plugins %>/check_users -w 25 -c 50
command[check_users_hot_and_spicy] = <%= @nrpe_plugins %>/check_users -w 50 -c 100
command[check_zombie_process] = <%= @nrpe_plugins %>/check_procs -w 5 -c 10 -s Z

# space
command[check_disk] = <%= @nrpe_plugins %>/check_disk -w 20% -c 10% -A -I 'var/lib/docker/containers'<% @check_disk_exclude_type.each do |type| -%> -X <%= type %><% end %>
command[check_disk_hot] = <%= @nrpe_plugins %>/check_disk -w 10% -c 5% -A -I 'var/lib/docker/containers'<% @check_disk_exclude_type.each do |type| -%> -X <%= type %><% end %>
command[check_disk_hot_and_spicy] = <%= @nrpe_plugins %>/check_disk -w 5% -c 1% -A -I 'var/lib/docker/containers'<% @check_disk_exclude_type.each do |type| -%> -X <%= type %><% end %>

# inode
