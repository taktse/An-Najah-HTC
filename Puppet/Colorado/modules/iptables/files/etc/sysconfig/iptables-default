-A INPUT -s 10.0.0.0/8 -p tcp -m state --state NEW -m tcp -j ACCEPT
-A INPUT -s 10.0.0.0/8 -p udp -m state --state NEW -m udp -j ACCEPT
-A INPUT -s 172.16.9.0/24 -p tcp -m state --state NEW -m tcp -j ACCEPT
-A INPUT -s 172.16.9.0/24 -p udp -m state --state NEW -m udp -j ACCEPT
-A INPUT -j REJECT --reject-with icmp-host-prohibited
